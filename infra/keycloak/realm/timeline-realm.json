{
  "realm": "timeline",
  "enabled": true,
  "sslRequired": "none",
  "displayName": "Timeline Planner",
  "displayNameHtml": "<strong>Timeline Planner</strong>",
  "loginTheme": "timeline",
  "registrationAllowed": true,
  "registrationEmailAsUsername": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "editUsernameAllowed": false,
  "verifyEmail": true,
  "resetPasswordAllowed": true,
  "rememberMe": true,
  "internationalizationEnabled": true,
  "supportedLocales": [
    "en",
    "ru"
  ],
  "defaultLocale": "en",
  "clients": [
    {
      "clientId": "timeline-supabase",
      "name": "Timeline Supabase",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "timeline-supabase-local-dev-secret",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "fullScopeAllowed": true,
      "attributes": {
        "post.logout.redirect.uris": "+"
      },
      "rootUrl": "http://localhost:5173",
      "baseUrl": "http://localhost:5173",
      "redirectUris": [
        "http://localhost:8080/auth/v1/callback",
        "http://127.0.0.1:8080/auth/v1/callback",
        "http://host.docker.internal:8080/auth/v1/callback",
        "http://localhost:5173/*",
        "http://127.0.0.1:5173/*",
        "http://host.docker.internal:5173/*"
      ],
      "webOrigins": [
        "http://localhost:5173",
        "http://127.0.0.1:5173",
        "http://host.docker.internal:5173"
      ]
    }
  ]
}
